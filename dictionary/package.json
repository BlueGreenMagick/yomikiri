{
  "name": "@yomikiri/dictionary",
  "collaborators": [
    "BlueGreenMagick <bluegreenmagick@gmail.com>"
  ],
  "description": "Yomikiri dictionary files",
  "version": "0.1.0",
  "license": "MIT",
  "exports": {
    "./*": "./resources/*"
  },
  "files": [
    "resources/english.yomikiridict",
    "resources/english.yomikiriindex",
    "resources/metadata.json"
  ],
  "scripts": {
    "_clean_pkg": "rm -rf ./pkg",
    "wasm": "pnpm run _clean_pkg && wasm-pack build --scope yomikiri --target web",
    "test": "cargo test",
    "check": "cargo check && cargo check --target wasm32-unknown-unknown",
    "lint": "cargo lint",
    "redownload": "rm -rf ./resources && cargo run",
    "construct": "cargo run --features bin && pnpm run wasm"
  }
}